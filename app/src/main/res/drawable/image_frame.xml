<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item>
        <shape android:shape="rectangle" >
            <gradient
                android:angle="360"
                android:centerColor="#FF3E98"
                android:endColor="#C00064"
                android:gradientRadius="360"
                android:startColor="#FF7DBB"
                android:type="linear" />

            <stroke
                android:width="2dp"
                android:color="#20C0BDBD" />
        </shape>
    </item>
    <item
        android:bottom="2dp"
        android:left="2dp"
        android:right="2dp"
        android:top="2dp">
        <shape android:shape="rectangle" >

        </shape>
    </item>

</layer-list>